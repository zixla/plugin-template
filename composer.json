{
    "name": ":lc:vendor/:lc:plugin",
    "description": ":plugin_description",
    "license": ":license",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_homepage"
        }
    ],
    "homepage": "https://github.com/:lc:vendor/:lc:plugin",
    "keywords": ["Laravel", ":uc:plugin"],
    "require": {
        "illuminate/support": "~7",
        "zixla/core": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            ":uc:vendor\\:uc:plugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":uc:vendor\\:uc:plugin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                ":uc:vendor\\:uc:plugin\\:uc:pluginServiceProvider"
            ],
            "aliases": {
                ":uc:plugin": ":uc:vendor\\:uc:plugin\\Facades\\:uc:plugin"
            }
        }
    }
}
